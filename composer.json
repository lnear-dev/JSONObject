{
  "name": "lnear/json",
  "description": "Lightweight JSON wrapper for PHP",
  "keywords": [
    "php",
    "json"
  ],
  "license": "MIT",
  "homepage": "https://github.com/lnear-dev/JSONObject",
  "authors": [
    {
      "name": "Lanre Ajao",
      "homepage": "https://github.com/lnear-dev",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1"
  },
  "autoload": {
    "psr-4": {
      "Lame\\": "src/"
    }
  },
  "scripts": {
    "test": "vendor/bin/pest",
    "test-coverage": "vendor/bin/pest --coverage"
  },
  "require-dev": {
    "pestphp/pest": "^3.0",
    "phpunit/phpunit": "^11.0"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "dev"
}
